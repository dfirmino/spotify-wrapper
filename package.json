{
  "name": "spotify-wrapper",
  "version": "1.0.0",
  "description": "Repositorio com testes",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/*.js --fix",
    "test": "mocha tests/**.spec.js --bail --require babel-register",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfirmino/spotify-wrapper"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "keywords": [
    "js",
    "tdd"
  ],
  "author": "Davi Firmino <dafirsouza@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfirmino/js-tdd/issues"
  },
  "homepage": "https://github.com/dfirmino/js-tdd#readme",
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0"
  }
}
